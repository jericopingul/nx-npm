#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nrwl+tao@14.3.6_p2756hm2bhhagtq775rcm3cefq/node_modules/@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../../../../../@nrwl+tao@14.3.6_p2756hm2bhhagtq775rcm3cefq/node_modules/@nrwl/tao/index.js" "$@"
fi
